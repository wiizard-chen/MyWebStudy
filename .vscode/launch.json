{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "request": "launch",
            "url": "${workspaceRoot}/JQuery_tutorial/AjaxTutorial.html",
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 8080,
            "webRoot": "${workspaceRoot}"
        }
    ]
}